{
    "plugins": ["prettier"],
    "extends": "solhint:recommended",
    "rules": {
        "prettier/prettier": "error",
        "max-line-length": ["warning", 140],
        "compiler-version": ["warning"],
        "no-global-import": ["warning"],
        "no-console": ["warning"],
        "private-vars-leading-underscore": ["warning"]
    }
}
