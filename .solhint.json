{
    "plugins": ["prettier"],
    "extends": "solhint:recommended",
    "rules": {
        "prettier/prettier": "error",
        "max-line-length": ["warn", 140],
        "compiler-version": "warn",
        "no-global-import": "warn",
        "no-console": "warn",
        "private-vars-leading-underscore": "warn",
        "custom-errors": "off"
    }
}
